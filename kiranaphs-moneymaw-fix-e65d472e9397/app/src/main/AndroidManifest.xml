<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.kiranapaulina.moneymaw1">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_catmaw"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_catmaw"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.JournalActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ui.ConverterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ui.DetailJouralActivity"
            android:screenOrientation="portrait"/>
    </application>
</manifest>
